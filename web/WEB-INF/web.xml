<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>applyCommunityServlet</servlet-name>
        <servlet-class>Controller.applyCommunityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GrantCommunityServlet</servlet-name>
        <servlet-class>Controller.GrantCommunityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>removeUserServlet</servlet-name>
        <servlet-class>Controller.removeUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>removeCommunityServlet</servlet-name>
        <servlet-class>Controller.removeCommunityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOutServlet</servlet-name>
        <servlet-class>Controller.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addCommunityServlet</servlet-name>
        <servlet-class>Controller.addCommunityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerCommunityServlet</servlet-name>
        <servlet-class>Controller.registerCommunityServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AskQues</servlet-name>
        <servlet-class>Controller.AskQues</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>applyCommunityServlet</servlet-name>
        <url-pattern>/applyCommunityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GrantCommunityServlet</servlet-name>
        <url-pattern>/GrantCommunityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>removeUserServlet</servlet-name>
        <url-pattern>/removeUserServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>removeCommunityServlet</servlet-name>
        <url-pattern>/removeCommunityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOutServlet</servlet-name>
        <url-pattern>/LogOutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addCommunityServlet</servlet-name>
        <url-pattern>/addCommunityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerCommunityServlet</servlet-name>
        <url-pattern>/registerCommunityServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AskQues</servlet-name>
        <url-pattern>/AskQues</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
